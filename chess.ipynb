   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import matplotlib as mpl\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "\n",
    "# date-time formatting\n",
    "from datetime import datetime\n",
    "import pytz\n",
    "\n",
    "# lichess python api\n",
    "import berserk as bk\n",
    "\n",
    "AUTH_TOKEN = ''\n",
    "\n",
    "session = bk.TokenSession(AUTH_TOKEN)\n",
    "client = bk.Client(session=session)"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Download data from lichess.org\n",
    "\n",
    "Returns 'iterators'. Put the data from the iterators into lists and combine them."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 109,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "832"
