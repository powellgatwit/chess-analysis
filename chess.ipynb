     "execution_count": 112,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "# currently, stalemates or draws are NaN under 'winner'\n",
    "# fix that\n",
    "non_wins = {\"winner\": 'tie'}\n",
    "g = g.fillna(non_wins)\n",
    "# check\n",
    "g['winner'].value_counts()"
   ]
  },
